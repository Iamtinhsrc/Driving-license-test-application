<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/category_graph"
    app:startDestination="@id/categoryFragment">

    <fragment
        android:id="@+id/categoryFragment"
        android:name="com.example.driving_car_project.category.CategoryFragment"
        android:label="@string/label_category">

        <action
            android:id="@+id/action_category_to_questions"
            app:destination="@id/question_graph">
            <argument
                android:name="categoryId"
                app:argType="integer" />
        </action>
    </fragment>
</navigation>
